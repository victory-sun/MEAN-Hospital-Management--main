    "crypto-js": "^4.0.0",
    "express": "^4.15.2",
    "fingerprintjs2": "^2.1.0",
    "moment": "^2.24.0",
    "moment-range": "^4.0.2",
    "ng-multiselect-dropdown": "^0.2.10",
    "ng-starrating": "^1.0.20",
