{
  "name": "caregiver",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prestart": "npm run extract-i18n && npm run i18n-po-to-json",
    "start": "ng serve --host=0.0.0.0",
    "prebuild:prod": "npm run extract-i18n && npm run i18n-po-to-json",
    "build:prod": "ng build --prod --vendor-chunk --common-chunk",
    "lint": "ng lint",
    "serve:ssr": "node dist/caregiver/server/main.js",
    "build:ssr": "ng build --prod && ng run caregiver:server:production",
    "i18n-po-to-json": "node i18n-po-to-json.js",
    "extract-i18n": "ngx-translate-extract --input ./src --output ./src/assets/i18n/zh.po ./src/assets/i18n/en.po --clean --format pot --key-as-default-value"
  },
  "private": true,
  "dependencies": {
